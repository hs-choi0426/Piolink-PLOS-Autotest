#S_CLI_LLDP_SET
interface {dev_port1}
set lldp enable txrx
end
show run interface {dev_port1}
conf
#E_CLI_LLDP_SET
#S_NBR1_LLDP_SET
interface {(nbr1)nbr_port1}
set lldp enable txrx
end
show run interface {(nbr1)nbr_port1}
conf
#E_NBR1_LLDP_SET
#S_CLI_LLDP_SET_SHOW
end
sleep 60
show lldp port {dev_port1}
conf
#E_CLI_LLDP_SET_SHOW
#S_NBR1_LLDP_SET_SHOW
end
show lldp port {(nbr1)nbr_port1}
conf
#E_NBR1_LLDP_SET_SHOW
#S_CLI_LLDP_UNSET_SHOW
interface {dev_port1}
set lldp disable
end
show lldp port {dev_port1}
show run interface {dev_port1}
conf
#E_CLI_LLDP_UNSET_SHOW
#S_NBR1_LLDP_UNSET_SHOW
interface {(nbr1)nbr_port1}
set lldp disable
end
show lldp port {(nbr1)nbr_port1}
show run interface {(nbr1)nbr_port1}
conf
#E_NBR1_LLDP_UNSET_SHOW
