#S_CLI_JUMBOFRAME_SET
interface {dev_port1}
jumbo-frame on
exit
show portstatus | include {dev_port1} 
#E_CLI_JUMBOFRAME_SET
#S_SHELL_JUMBOFRAME_SET
rtk_user_diag
switch get max-pkt-len port {sdk_port1} ge length
exit
#E_SHELL_JUMBOFRAME_SET
#S_CLI_JUMBOFRAME_UNSET
interface {dev_port1}
jumbo-frame off
exit
show portstatus | include {dev_port1} 
#E_CLI_JUMBOFRAME_UNSET
#S_SHELL_JUMBOFRAME_UNSET
rtk_user_diag
switch get max-pkt-len port {sdk_port1} ge length
exit
#E_SHELL_JUMBOFRAME_UNSET
