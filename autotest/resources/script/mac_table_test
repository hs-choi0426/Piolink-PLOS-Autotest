#S_CLI_MAC_TABLE_SET
interface {output_port}
shutdown
end
clear mac address-table dynamic
clear arp
sleep 10
show mac-table | include {input_port} 
configure terminal force
#E_CLI_MAC_TABLE_SET
#S_SHELL_MAC_TABLE_SET
rtk_user_diag
l2-table dump mac-ucast all
exit
#E_SHELL_MAC_TABLE_SET
#S_CLI_MAC_TABLE_UNSET
interface {output_port}
no shutdown
exit
#E_CLI_MAC_TABLE_UNSET
