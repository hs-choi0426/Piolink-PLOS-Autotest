bash-4.4# #S_NBR1_PING_UNSET
TiFRONT(config)# 
interface range ge42,ge41
TiFRONT(config-if-range)# 
no shutdown
TiFRONT(config-if-range)# 
interface vlan100
TiFRONT(config-if-vlan100)# 
no ip addr 10.10.10.2/24 
TiFRONT(config-if-vlan100)# 
exit
TiFRONT(config)# 
