#S_CLI_ARP_DYNAMIC_SHOW
TiFRONT(config)# 
ip forwarding
TiFRONT(config)# 
vlan 500
TiFRONT(config)# 
interface vlan500
TiFRONT(config-if-vlan500)# 
ip address 20.0.0.1/24
TiFRONT(config-if-vlan500)# 
interface range ge17,ge18
TiFRONT(config-if-range)# 
switchport access vlan 500
TiFRONT(config-if-range)# 
end
