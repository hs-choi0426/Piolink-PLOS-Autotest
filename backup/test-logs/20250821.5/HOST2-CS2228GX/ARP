bash-4.4# #S_NBR1_ARP_TIMEOUT_SET
TiFRONT(config)# 
interface ge41
TiFRONT(config-if-ge41)# 
no shutdown
TiFRONT(config-if-ge41)# 
exit
TiFRONT(config)# 
#E_NBR1_ARP_TIMEOUT_SET
TiFRONT(config)# #S_CLI_ARP_TIMEOUT_SHOW
TiFRONT(config)# 
no mac ageing-time 
TiFRONT(config)# 
interface vlan500
TiFRONT(config-if-vlan500)# 
arp-ageing-timeout 60
TiFRONT(config-if-vlan500)# 
end
