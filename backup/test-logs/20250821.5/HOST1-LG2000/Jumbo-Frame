#S_CLI_JUMBOFRAME_SET
TiFRONT(config)# 
interface ge5
TiFRONT(config-if-ge5)# 
jumbo-frame on
TiFRONT(config-if-ge5)# 
exit
TiFRONT(config)# 
show portstatus | include ge5 
      ge5 |  EN |  UP  | AUTO |  1000 | FULL |  EN |  FWD  |  CO 
TiFRONT(config)# 
#E_CLI_JUMBOFRAME_SET
TiFRONT(config)# #S_SHELL_JUMBOFRAME_SET
bash-4.4# 
rtk_user_diag
RTK.0> 
switch get max-pkt-len port 33 ge length
Max Packet Length in Giga Speed
Port 33 : 12288
RTK.0> 
exit
RTK.0> 
bash-4.4# 
#E_SHELL_JUMBOFRAME_SET
bash-4.4# #S_CLI_JUMBOFRAME_UNSET
TiFRONT(config)# 
interface ge5
TiFRONT(config-if-ge5)# 
jumbo-frame off
TiFRONT(config-if-ge5)# 
exit
TiFRONT(config)# 
show portstatus | include ge5 
      ge5 |  EN |  UP  | AUTO |  1000 | FULL | DIS |  FWD  |  CO 
TiFRONT(config)# 
#E_CLI_JUMBOFRAME_UNSET
