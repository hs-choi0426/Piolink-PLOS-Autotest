TiFRONT(config)# 
interface ge3
TiFRONT(config-if-ge3)# 
no shutdown
TiFRONT(config-if-ge3)# 
interface ge5
TiFRONT(config-if-ge5)# 
switchport mode access
TiFRONT(config-if-ge5)# 
switchport access vlan 100
TiFRONT(config-if-ge5)# 
interface ge7
TiFRONT(config-if-ge7)# 
switchport mode access
TiFRONT(config-if-ge7)# 
switchport access vlan 200
TiFRONT(config-if-ge7)# 
interface xg1
TiFRONT(config-if-xg1)# 
switchport mode access
TiFRONT(config-if-xg1)# 
switchport access vlan 300
TiFRONT(config-if-xg1)# 
exit
TiFRONT(config)# 
