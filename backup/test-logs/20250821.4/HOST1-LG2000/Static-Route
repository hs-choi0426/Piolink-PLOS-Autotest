interface vlan1000
TiFRONT(config-if-vlan1000)# 
ip addr 192.168.197.201/24
TiFRONT(config-if-vlan1000)# 
interface ge1
TiFRONT(config-if-ge1)# 
no shutdown
TiFRONT(config-if-ge1)# 
exit
TiFRONT(config)# 
ip route 0.0.0.0/0 192.168.197.1
TiFRONT(config)# 
exit
